<templateSet group="surround">
  <template name="ef" value="// &lt;editor-fold desc=&quot;$Title$&quot;&gt;" description="Editor Fold" toReformat="false" toShortenFQNames="true">
    <variable name="Title" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="false" />
      <option name="OTHER" value="true" />
      <option name="XML" value="false" />
    </context>
  </template>
  <template name="eef" value="// &lt;/editor-fold&gt;" description="Close Editor Fold" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="xr" value="&lt;!--region $Name$--&gt;" description="XML Editor Fold/Region" toReformat="false" toShortenFQNames="true">
    <variable name="Name" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="XML" value="true" />
    </context>
  </template>
  <template name="exr" value="&lt;!--endregion--&gt;" description="XML Editor Fold/Region" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="XML" value="true" />
    </context>
  </template>
</templateSet>