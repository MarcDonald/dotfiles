<keymap version="1" name="Default copy" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="shift ctrl back_space" />
  </action>
  <action id="JumpToLastChange" />
  <action id="ReplaceInPath" />
  <action id="Rerun">
    <keyboard-shortcut first-keystroke="ctrl f5" />
    <keyboard-shortcut first-keystroke="shift ctrl r" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="shift f10" />
    <keyboard-shortcut first-keystroke="alt r" />
  </action>
</keymap>