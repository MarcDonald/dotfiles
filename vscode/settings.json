{
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"editor.fontSize": 13,
	"editor.fontLigatures": true,
	"editor.fontFamily": "'Comic Code Ligatures', 'JetBrainsMono Nerd Font', Consolas, 'Courier New', monospace",
	"editor.tabSize": 2,
	"files.autoSave": "onFocusChange",
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"editor.foldingImportsByDefault": true,
	"editor.smoothScrolling": true,
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.cursorSurroundingLines": 2,
	"editor.find.autoFindInSelection": "always",
	"workbench.colorTheme": "One Dark Pro",
	"debug.console.fontSize": 15,
	"eslint.format.enable": true,
	"eslint.packageManager": "npm",
	"reactSnippets.settings.prettierEnabled": true,
	"git.autofetch": true,
	"markdown.preview.fontSize": 15,
	"material-icon-theme.activeIconPack": "react",
	"npm.packageManager": "npm",
	"npm.enableRunFromFolder": true,
	"prettier.configPath": ".prettierrc.cjs",
	"vim.camelCaseMotion.enable": true,
	"vim.easymotion": true,
	"vim.easymotionMarkerForegroundColorOneChar": "#ffffff",
	"vim.easymotionMarkerForegroundColorTwoCharFirst": "#FFFF00",
	"vim.easymotionMarkerForegroundColorTwoCharSecond": "#ffffff",
	"vim.highlightedyank.enable": true,
	"vim.leader": "<space>",
	"vim.showMarksInGutter": true,
	"vim.smartRelativeLine": true,
	"vim.sneak": true,
	"vim.sneakUseIgnorecaseAndSmartcase": true,
	"vim.statusBarColorControl": true,
	"vim.useSystemClipboard": true,
	"vim.visualstar": true,
	"workbench.colorCustomizations": {
		"statusBar.background": "#005f5f",
		"statusBar.noFolderBackground": "#005f5f",
		"statusBar.debuggingBackground": "#005f5f"
	},
	"vim.normalModeKeyBindings": [
		{
			"before": ["leader", "q"],
			"commands": ["workbench.action.closeActiveEditor"]
		},
		{
			"before": ["leader", "r", "n"],
			"commands": ["editor.action.rename"]
		},
		{
			"before": ["leader", "g", "d"],
			"commands": ["editor.action.revealDefinition"]
		},
		{
			"before": ["leader", "a", "j"],
			"commands": ["editor.action.marker.nextInFiles"]
		},
		{
			"before": ["leader", "a", "k"],
			"commands": ["editor.action.marker.prevInFiles"]
		},
		{
			"before": ["C-w", "s"],
			"commands": ["workbench.action.splitEditor"]
		},
		{
			"before": ["C-w", "v"],
			"commands": ["workbench.action.splitEditor"]
		},
		{
			"before": ["C-w", "h"],
			"commands": ["workbench.action.splitEditorDown"]
		},
		{
			"before": ["Q"],
			"after": ["g", "q"]
		},
		{
			"before": ["leader", "l"],
			"after": ["g", "t"]
		},
		{
			"before": ["leader", "h"],
			"after": ["g", "T"]
		},
		{
			"before": ["leader", "c"],
			"commands": ["workbench.action.showCommands"]
		},
		{ "before": ["leader", "w"], "commands": ["workbench.action.files.save"] },
		{
			"before": ["leader", "f"],
			"after": ["leader", "leader"]
		}
	],
	"vim.handleKeys": {
		"<C-d>": true,
		"<C-a>": false,
		"<C-r>": false,
		"<C-f>": false,
		"Shift-Alt-F": false,
		"Shift-F": false
	},
	"vim.insertModeKeyBindings": [],
	"workbench.preferredDarkColorTheme": "One Dark Pro",
	"workbench.preferredLightColorTheme": "GitHub Light Default",
	"window.autoDetectColorScheme": true,
	"terminal.integrated.fontSize": 13,
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.formatOnSave": true,
	"workbench.startupEditor": "none",
	"vim.neovimUseConfigFile": true,
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.patterns": {
		"*.ts": "${capture}.js",
		"*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
		"*.jsx": "${capture}.js",
		"*.tsx": "${capture}.ts",
		"tsconfig.json": "tsconfig.*.json",
		"package.json": "package-lock.json, yarn.lock",
		"*.scss": "${capture}.scss, ${capture}.css, ${capture}.css.map"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"npm.keybindingsChangedWarningShown": true,
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": "keyword.other.dotenv",
				"settings": {
					"foreground": "#FF000000"
				}
			}
		]
	},
	"vim.vimrc.enable": true,
	"editor.stickyScroll.enabled": true,
	"[javascript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"workbench.editor.wrapTabs": true,
	"screencastMode.fontSize": 30,
	"screencastMode.onlyKeyboardShortcuts": true,
	"screencastMode.verticalOffset": 10,
	"window.nativeTabs": true,
	"diffEditor.codeLens": true,
	"explorer.compactFolders": false,
	"github.gitProtocol": "ssh",
	"git.enableCommitSigning": true,
	"git.autoStash": true,
	"git.branchProtection": ["main", "master"],
	"git.allowForcePush": true,
	"git.defaultCloneDirectory": "$HOME/Documents/misc",
	"git.showPushSuccessNotification": true,
	"gitlens.autolinks": [
		{
			"ignoreCase": false,
			"prefix": "UTSS-",
			"url": "https://forge.lmig.com/issues/browse/UTSS-<num>"
		},
		{
			"ignoreCase": false,
			"prefix": "LITIA-",
			"url": "https://forge.lmig.com/issues/browse/LITIA-<num>"
		},
		{
			"ignoreCase": false,
			"prefix": "CICC-",
			"url": "https://forge.lmig.com/issues/browse/CICC-<num>"
		},
		{
			"prefix": "DMR-",
			"url": "https://forge.lmig.com/issues/browse/DMR-<num>"
		}
	],
	"aws.samcli.enableCodeLenses": true,
	"vim.hlsearch": true,
	"vim.vimrc.path": "/Users/marc/.vscodevimrc",
	"gitlens.views.branches.branches.layout": "list",
	"editor.linkedEditing": true,
	"boot-java.rewrite.reconcile": true,
	"[markdown]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.wordWrap": "off"
	},
	"githubPullRequests.pullBranch": "never",
	"git.pruneOnFetch": true,
	"javascript.inlayHints.functionLikeReturnTypes.enabled": true,
	"typescript.format.semicolons": "insert",
	"typescript.inlayHints.parameterNames.enabled": "all",
	"editor.minimap.enabled": false,
	"gitlens.views.commits.showBranchComparison": false,
	"gitlens.views.commits.avatars": false,
	"gitlens.views.commits.pullRequests.showForBranches": false,
	"gitlens.views.commits.pullRequests.enabled": false,
	"editor.wordWrapColumn": 120,
	"workbench.iconTheme": "catppuccin-mocha",
	"prettier.useTabs": true,
	"files.exclude": {
		".idea": true,
		"*.iml": true
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnPaste": true,
	"prettier.jsxSingleQuote": true,
	"workbench.productIconTheme": "material-product-icons",
	"window.zoomLevel": 1,
	"terminal.external.osxExec": "iTerm2.app",
	"terminal.integrated.fontFamily": "'Comic Code Ligatures', 'JetBrainsMono Nerd Font'",
	"editor.tabCompletion": "on"
}
